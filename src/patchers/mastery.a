;-------------------------------
; #MASTERY
; RWTS changes based on track
;
; tested on
; - Mastery Arithmetic Games v1.2 (Mastery Development)
;-------------------------------
!zone {
         lda   gIsRWTS   ; if DOS 3.3-shaped RWTS
         bne   .exit
         lda   #$07
         ldx   #$00
         ldy   #$03
         jsr   compare
         !byte $20,$AF,$BE
         bcs   .exit
         lda   #$08
         ldx   #$AF
         ldy   #$0B
         jsr   compare
         !byte $84,$48
         !byte $85,$49
         !byte $A0,$04
         !byte $B1,$48
         !byte $C9,$03
         !byte $90
         bcs   .exit
         jsr   PrintByID
         !byte s_bytrack
         ldx   #$B8
         ldy   #$01
         jsr   modify
         !byte $23
.exit
}
