;-------------------------------
; #A5COUNT
; nibble count between $A5 and
; address prologue
; e.g. Game Frame One, Game Frame Two
;-------------------------------
!zone {
_a5count
         lda   gIsPascal  ; only ever seen this protection
         bne   .exit      ; on Pascal disks

         ldy   #$0A
         jsr   SearchTrack
         !byte $07
         !byte $E6,$02    ; INC $02
         !byte $D0,$03    ; BNE +3
         !byte $4C,$A5,$00; JMP $00A5
         !byte $C9,$A5    ; CMP #$A5
         bcs   .exit
         sta   gDisplayBytes
         pha
         lda   #s_a5count
         jsr   PrintByID
         txa
         clc
         adc   #$08
         tax
         pla
         ldy   #$02
         jsr   modify
         !byte $D0,$7B    ; BNE +$7B
.exit
}
