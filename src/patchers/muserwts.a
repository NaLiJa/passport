;-------------------------------
; #MUSERWTS
; RWTS changes based on track
;
; tested on
; - The Caverns of Freitag (MUSE)
; - The Function Game (MUSE)
;-------------------------------
!zone {
         lda   gIsMUSERWTS
         bne   .exit
         lda   #$06
         ldx   #$F2
         ldy   #$03
         jsr   compare    ; if T00,S06,$F2 ==
         !byte $20,$B6,$B6
         bcs   .exit      ; passport-test-suite/The Function Game.woz [C=0] matches
         sta   gDisplayBytes
         jsr   PrintByID
         !byte s_bytrack
         ldy   #$01
         jsr   modify     ; then set T00,S06,$F2 =
         !byte $2C
.exit
}
