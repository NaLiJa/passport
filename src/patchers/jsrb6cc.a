;-------------------------------
; #JSRB6CC
; RWTS changes based on track
;
; tested on
; - KidWriter (Spinnaker Software)
;-------------------------------
!zone {
         lda   gIsRWTS   ; if DOS 3.3-shaped RWTS
         bne   .exit
         lda   #$07
         ldx   #$00
         ldy   #$03
         jsr   compare
         !byte $20,$CC,$B6
         bcs   .exit
         lda   #$00
         ldx   #$CC
         ldy   #$08
         jsr   compare
         !byte $84,$48
         !byte $85,$49
         !byte $AD,$EC,$B7
         !byte $F0
         bcs   .exit
         sta   gDisplayBytes
         lda   #s_bytrack
         jsr   PrintByID
         lda   #$00
         ldx   #$D1
         ldy   #$01
         jsr   modify
         !byte $FB
.exit
}
