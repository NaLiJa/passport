;-------------------------------
; #ARTSCI
; loader on T00,S0A changes epilogue
;
; tested on
; - AceCalc (ArtSci)
; - Magicalc (ArtSci)
; - Magic Words
; - Photar
;-------------------------------
!zone {
         ; gTrack = 0
         ; Caller found DOS 3.3 RWTS

         lda   #$0A
         ldx   #$FE
         ldy   #$02
         jsr   compare
         !byte $DE,$D5
         bcs   .exit

         ldy   #(@searchend-@searchstart)
         jsr   SearchTrack
@searchstart
         LDY   #$35
         TXA
         STA   ($3C),Y
@searchend
         bcs   .exit      ; passport-test-suite/Photar.woz [C=0] matches
         jsr   PrintByID
         !byte s_bytrack
         ldy   #$01
         jsr   modify
         !byte $60
.exit
}
