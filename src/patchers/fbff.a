;-------------------------------
; #FBFF
; Activision's Timing Bit Detection:
;  FB+FF FF+FF FF+FD FF+FF+FF+FF+FF+FF+FF+FF+FF+ and
;  DE AA EB+FF+FF+FF+FF+FF+FF D5 AA
; Magic key is #$FF, usually EOR'ed with #$AA but not always
; As found on Aliens, Maniac Mansion + 22 other titles
;
; Module by Brian Troha
;-------------------------------
!zone {
         bit   gMode      ; nothing to do here in verify-only mode
         bpl   .exit
         ldy   #$14
         jsr   SearchTrack
         !byte $BD,$89,$C0
         !byte $A9,$56
         !byte $85,WILDCARD
         !byte $A9,$08
         !byte $C6,WILDCARD
         !byte $D0,$04
         !byte $C6,WILDCARD
         !byte $F0,WILDCARD
         !byte $BC,$8C,$C0
         bcs   .exit
         sta   gDisplayBytes
         pha
         lda   #s_fbff
         jsr   PrintByID
         pla
         ldy   #$05
         jsr   modify
         !byte $A9,$FF
         !byte $18
         !byte $90,$40
.exit
}
