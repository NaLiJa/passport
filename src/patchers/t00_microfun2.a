;-------------------------------
; #MICROFUN2
; RWTS jumps to $B6B8 to burn cycles during write
;
; tested on
; - Crown Of Arthain
;-------------------------------
!zone {
         ; gTrack = 0
         ; Caller found DOS 3.3 RWTS

         lda   #$02
         ldx   #$5E
         ldy   #$03
         jsr   compare
         !byte $4C,$B8,$B6
         bcs   .exit      ; passort-test-suite/Crown Of Arthain.woz [C=0] matches
         jsr   modify
         !byte $20,$B8,$B8
.exit
}
