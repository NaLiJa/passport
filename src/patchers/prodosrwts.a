;-------------------------------
; #PRODOSRWTS
; patch ProDOS RWTS
; e.g. ProDOS-based MECC disks
;-------------------------------
!zone {
_prodosrwts
         ; gIsProDOS is TRUE here

         ldy   #$14
         jsr   SearchTrack
         !byte $29,$FC
         !byte $5D,WILDCARD,WILDCARD
         !byte $D0,$0C
         !byte $A6,WILDCARD
         !byte $BD,$8C,$C0
         !byte $10,$FB
         !byte $C9,WILDCARD
         !byte $18
         !byte $F0,$01
         !byte $38
         bcs   .exit      ; passport-test-suite/Mystery Objects v1.1.woz [C=0] matches
         jsr   inx0F
         ldy   #$01
         jsr   compare
         !byte $DE
         bcc   .exit
         jsr   modify
         !byte $DE
.exit
}
