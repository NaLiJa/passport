;-------------------------------
; #JMPB720
; RWTS swapper forcibly reinstalls
; a jump to $B720 instead of writing
; third data prologue
; e.g. Analogies Tutorial, Verb Viper
;-------------------------------
!zone {
_jmpb720
         bit   gMode      ; nothing to do here in verify-only mode
         bpl   .exit
         lda   gIsRWTS    ; if DOS 3.3-shaped RWTS
         bne   .exit
         lda   #$0B
         ldx   #$08
         ldy   #$18
         jsr   compare    ; and T00,S0B,$08 ==
         !byte $4C,$20,$B7
         !byte $4C,$0E,$A9
         !byte $A2,$02
         !byte $BD,$0B,$B7
         !byte $9D,$5E,$B8
         !byte $BD,$08,$B7
         !byte $9D,$F6,$B8
         !byte $CA
         !byte $10,$F1
         !byte $60
         bcs   .exit
         lda   #$0B
         ldx   #$08
         ldy   #$06
         jsr   modify     ; then set T00,S0B,$08 =
         !byte $BD,$8C,$C0
         !byte $20,$B8,$B8
.exit
}
