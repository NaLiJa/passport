!zone {
         ldx   gTrack
         dex
         bne   .exit

         lda   #$01
         ldx   #$95
         ldy   #$03
         jsr   compare    ; if T01,S01,$95 ==
         !byte $4C,$32,$A3
         bcs   .exit      ; passport-test-suite/Harvey by Primes.woz [C=0] here

         ldy   #$A2
         sty   gDisplayBytes+1       ; address (hi)
         ldy   #$82
         sty   gDisplayBytes+2       ; address (lo)
         jsr   PrintByID
         !byte s_beforedos

         inx
         tay
         jsr   modify     ; then set T01,S01,$96 =
         !byte $47
.exit
}
