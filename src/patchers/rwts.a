;-------------------------------
         lda   gIsRWTS    ; if DOS 3.3 RWTS
         bne   .lsr6a
         lda   #$03
         sta   gDisplayBytes
         ldx   #$4F
         ldy   #$0A
         jsr   compare    ; and T00,S03,$4F ==
         !byte $BD,$8C,$C0,$10,$FB,$4A,$97,$6A,$D0,$EF
         bcs   .lsr6a
         lda   #s_lsr6a
         jsr   PrintByID
         inc   gPatchCount
.lsr6a

