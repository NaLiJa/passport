         bne   .bcs08
         lda   #$03
         sta   gDisplayBytes
         ldx   #$8B
         ldy   #$15
         jsr   compare    ; and T00,S03,$8B ==
         !byte $BD,$8C,$C0,$10,$FB,$C9,$DE,$F0,$0A,$48,$68,$BD,$8C,$C0,$C9,$08,$B0,$A5,$EA,$18,$60
         bcs   .bcs08
         lda   #s_bcs08
         jsr   PrintByID
