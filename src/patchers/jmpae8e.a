;-------------------------------
; #JMPAE8E
; Late stage DOS jumps to $AE8E
; for a nibble check
; e.g. Alien Addition
;-------------------------------
!zone {
         lda   gIsBoot0   ; if DOS 3.3 boot0 loader
         ora   gIsBoot1   ; and DOS 3.3 boot1 loader
         bne   .exit
         lda   #$0D
         ldx   #$4D
         ldy   #$03
         jsr   compare    ; and T00,S0D,$4D ==
         !byte $4C,$8E,$AE
         bcs   .exit
         lda   #s_jmpae8e
         jsr   PrintByID
         lda   #$0D
         inx
         ldy   #$02
         jsr   modify     ; then set T00,S0D,$4E =
         !byte $80,$A1
.exit
}
