;-------------------------------
; #RWTSSWAPMECC
; post-boot RWTS swapper in MECC
; fastloader disks
;-------------------------------
!zone {
         bit   gMode      ; nothing to do here in verify-only mode
         bpl   .exit
         lda   gMECCFastloadType
         beq   .exit

         ldy   #$08
         jsr   SearchTrack
         !byte $A0,$02
         !byte $AD,$88,$67
         !byte $D0,$11
         !byte $B9
         bcs   +          ; passport-test-suite/MECC Dataquest Composer [C=0] matches
         sta   gDisplayBytes
         jsr   inx5
         ldy   #$01
         jsr   modify
         !byte $24
+
         ldy   #$11
         jsr   SearchTrack
         !byte $0A
         !byte $18
         !byte $69,$FF
         !byte $A8
         !byte $8D,$8B,$C0
         !byte $8D,$8B,$C0
         !byte $B9,WILDCARD,WILDCARD
         !byte $8D,$AC,$D5
         bcs   .exit      ; passport-test-suite/MECC Dataquest - Europe and Soviet Union.woz [C=0] matches
         sta   gDisplayBytes
         ldy   #$03
         jsr   modify
         !byte $A9,$00
         !byte $24
.exit
}
