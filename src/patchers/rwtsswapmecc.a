;-------------------------------
; #RWTSSWAPMECC
; post-boot RWTS swapper in MECC
; disks
; TODO might be able to gate this on MECC fastloader
; (I think it's only used on Dataquest disks)
;-------------------------------
!zone {
_rwtsswapmecc
         ldy   #$11
         jsr   SearchTrack
         !byte $0A
         !byte $18
         !byte $69,$FF
         !byte $A8
         !byte $8D,$8B,$C0
         !byte $8D,$8B,$C0
         !byte $B9,WILDCARD,WILDCARD
         !byte $8D,$AC,$D5
         bcs   .exit
         sta   gDisplayBytes
         ldy   #$03
         jsr   modify
         !byte $A9,$00
         !byte $24
.exit
}
