!zone {
         lda   gIsBoot0
         bne   .exit
         ldx   gTrack
         dex
         bne   .exit

         ldy   #(@search1end-@search1start)
         jsr   SearchTrack
@search1start
         LDX   $B7E9
         LDA   $C089,X
         LDA   $C08E,X
         LDA   #$00
@search1end
         bcs   .exit

         ldy   #(@search2end-@search2start)
         jsr   SearchTrack
@search2start
         !byte $20,WILDCARD,WILDCARD
         !byte $20,WILDCARD,WILDCARD
         LDA   #$00
         STA   $10
         LDA   #$20
         STA   $11
         !byte $20
@search2end
         bcs   .exit      ; passport-test-suite/Super Taxman II.woz [C=0] matches
         jsr   PrintByID
         !byte s_hallabs
         ldy   #$01
         jsr   modify
         RTS
.exit
}
