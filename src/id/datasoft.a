         !byte $10,$41,$00,$23,$80,$5B,$0A,$10
         !byte $0B,$4E,$9D,$0A,$10,$9D,$0C,$10
         !byte $60,$1E,$53,$10,$90,$53,$BC,$90
         !byte $53,$00,$90,$D8,$52,$00,$D8,$7C
         !byte $00,$53,$80,$0B,$06,$41,$00,$09
