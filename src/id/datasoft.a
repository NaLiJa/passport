         lda   #$00
         tax
         ldy   #$7E
         jsr   compare
         !byte $01,$4C,$7E,$08,$04,$8A,$0C,$B8
         !byte $00,$56,$10,$7A,$00,$00,$1A,$16
         !byte $12,$0E,$0A,$06,$53,$18,$9A,$02
